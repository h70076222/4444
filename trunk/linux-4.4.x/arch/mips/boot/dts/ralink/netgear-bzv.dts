/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "Netgear BZV";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		wps {
			label = "wps";
			gpios = <&gpio0 17 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "wlan";
			gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		fn1 { /* wlan */
			label = "fn1";
			gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_FN_1>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0 0x100000>;		/* 1m */
			};

			partition@100000 {
				label = "SC_PART";
				reg = <0x100000 0x100000>;		/* 1m */
			};

			partition@200000 {
				label = "firmware";
				reg = <0x200000 0x2c00000>;		/* 44m */
			};

			partition@2e00000 {
				label = "reserved0";
				reg = <0x2e00000 0x1800000>;	/* 24m */
			};

			partition@4600000 {
				label = "Factory";
				reg = <0x4600000 0x200000>;		/* 2m */
			};

			partition@4800000 {
				label = "reserved1";
				reg = <0x4800000 0xc00000>;		/* 12m */
			};

			partition@5400000 {
				label = "Config";
				reg = <0x5400000 0x80000>;		/* 512k */
			};

			partition@5480000 {
				label = "Storage";
				reg = <0x5480000 0x400000>;		/* 4m */
			};

			partition@5880000 {
				label = "RWFS";
				reg = <0x5880000 0x2700000>;	/* 39m */
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&xhci {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};
